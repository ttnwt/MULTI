node('built-in') 
{
    stage('Continue Download') 
    {
        git branch: 'pipeline1', credentialsId: 'github-user', url: 'https://github.com/ttnwt/webapp.git'
}
stage('Continue Build') 
    {
       sh 'mvn package'
}
stage('Continue Deployment') 
    {
       deploy adapters: [tomcat8(credentialsId: 'QA', path: '', url: 'http://172.31.89.178:8080')], contextPath: 'qaenv', war: '**/*.war'
}
stage('Continue Testing') 
    {
       sh 'echo "testing is successful"'
}
stage('Continue Production') 
    {
       deploy adapters: [tomcat8(credentialsId: 'github-user1', path: '', url: 'http://172.31.4.113:8080')], contextPath: 'devenv', war: '**/*.war'
}
}
